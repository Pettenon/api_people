<h1>Listing degrees</h1>

<table>
  <tr>
    <th>People importer</th>
    <th>Degree type</th>
    <th>Subject</th>
    <th>Institution</th>
    <th>Graduated year</th>
    <th>Graduated month</th>
    <th>Graduated day</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @degrees.each do |degree| %>
  <tr>
    <td><%= degree.people_importer_id %></td>
    <td><%= degree.degree_type %></td>
    <td><%= degree.subject %></td>
    <td><%= degree.institution %></td>
    <td><%= degree.graduated_year %></td>
    <td><%= degree.graduated_month %></td>
    <td><%= degree.graduated_day %></td>
    <td><%= link_to 'Show', degree %></td>
    <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
    <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Degree', new_degree_path %>
